    "cookie-parser": "^1.4.3",
    "debug": "^2.1.3",
    "dotenv": "^2.0.0",
    "eslint": "^2.13.1",
    "express": "^4.17.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.11.3",
    "istanbul": "^0.4.5",
    "jade": "^1.11.0",
    "jsonwebtoken": "^5.4.1",
    "mocha": "^2.5.3",
    "morgan": "^1.5.3",
    "npm": "^5.7.1",
    "omit": "^1.0.1",
    "passport": "^0.3.2",
    "passport-saml": "^0.15.0",
